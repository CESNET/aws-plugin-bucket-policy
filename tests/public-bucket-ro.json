{
  "Id": "policy-TEST",
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "statement-TEST",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::test_bucket",
        "arn:aws:s3:::test_bucket/*"
      ]
    }
  ]
}